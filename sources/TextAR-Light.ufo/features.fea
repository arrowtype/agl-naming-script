        sub [teh-ar.medi.wide theh-ar.medi.wide teh-ar.init.wide theh-ar.init.wide yehHamzaabove-ar.medi.wide yehHamzaabove-ar.init.wide tteh-ar.medi.wide tteh-ar.init.wide] [teh-ar.medi theh-ar.medi yehHamzaabove-ar.medi tteh-ar.medi tteh-ar.init]' by [teh-ar.medi.wide theh-ar.medi.wide yehHamzaabove-ar.medi.wide tteh-ar.medi.wide tteh-ar.init.wide ];
        sub [alef-ar alef-ar.fina alefHamzaabove-ar alefHamzaabove-ar.fina alefHamzabelow-ar alefHamzabelow-ar.fina alefMadda-ar alefMadda-ar.fina alefWasla-ar alefWasla-ar.fina] [teh-ar.init theh-ar.init yehHamzaabove-ar.init tteh-ar.init]' by [teh-ar.init.wide theh-ar.init.wide yehHamzaabove-ar.init.wide tteh-ar.init.wide];
    } teh_wide;

    lookup yeh_wide {
        sub [beh-ar.init yeh-ar.init peh-ar.init beh-ar.medi yeh-ar.medi heh-ar.medi] [yeh-ar.medi peh-ar.medi]' by [yeh-ar.medi.wide peh-ar.medi.wide];
        sub [yeh-ar.medi yeh-ar.init peh-ar.medi peh-ar.init]' [beh-ar.medi yeh-ar.medi heh-ar.medi ain-ar.fina] by [yeh-ar.medi.wide yeh-ar.init.wide peh-ar.medi.wide peh-ar.init.wide];
