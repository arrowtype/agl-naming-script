    } yeh_wide;

} calt;


# --------------------
# Case-Sensitive Forms
# --------------------

feature case {
    # Capital version of middotcat
    sub middotcat by middotcat.cap;
} case;


# --------------
# Stylistic Sets
# --------------

# Duplicating locl feature
feature ss01 { 
    featureNames {
        name "Localized Forms (Urdu)"; # Windows English
        name 1 0 0 "Localized Forms (Urdu)"; # Mac English
    };
    lookup UrduSix {
        sub six-persian by six-ar;
        sub six-persian.tab by six-ar.tab;
    } UrduSix;
} ss01;

feature ss11 {
    featureNames {
        name "Raised Colon (Figures Only)"; # Windows English
        name 1 0 0 "Raised Colon (Figures Only)"; # Mac English
    };
    lookup FigureColonContextual {
        sub @figD colon' @figD by colon.fig;
        sub @figTab colon.tab' @figTab by colon.figtab;
    } FigureColonContextual;
} ss11;

feature ss12 {
    featureNames {
        name "Raised Colon (Global)"; # Windows English
        name 1 0 0 "Raised Colon (Global)"; # Mac English
    };
    lookup FigureColon {
        sub colon by colon.fig;
        sub colon.tab by colon.figtab;
    } FigureColon;
} ss12;


# --------------------
# Stylistic Alternates
# --------------------

feature salt {
    lookup UrduSix; #SS01
    lookup FigureColon; # SS12
} salt;


# ---------------
# Tabular Figures
# ---------------

feature tnum { 
    # Arabic/Persian
    sub @arFigD by @arFigTab;
    sub @arFigTab space' @arFigTab by space.tab;

    # Latin
    sub @figD by @figTab;
    sub @currD by @currTab;

    @tab = [@figTab @currTab];

    lookup tabPX {
        sub @tab @figPxD' @tab by @figPxTab;

        sub @tab @figPxD' @figPxD @tab by @figPxTab;
        sub @tab @figPxTab @figPxD' @tab by @figPxTab;

        sub @tab @figPxD' @figPxD @figPxD @tab by @figPxTab;
        sub @tab @figPxTab @figPxD' @figPxD @tab by @figPxTab;
        sub @tab @figPxTab @figPxTab @figPxD' @tab by @figPxTab;

    } tabPX;
} tnum;



# --------------------
# Proportional Figures
# --------------------

feature pnum {
    # Arabic/Persian
    sub @arFigTab by @arFigD;
    sub space.tab by space;

    # Latin
    sub @figTab by @figD;
    sub @currTab by @currD;
    sub @figPxTab by @figPxD;
} pnum;



